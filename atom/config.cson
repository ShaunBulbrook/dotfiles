"*":
  welcome:
    showOnStartup: false
  core:
    disabledPackages: [
      "wrap-guide"
    ]
    autoHideMenuBar: true
    themes: [
      "one-dark-ui"
      "atom-material-syntax"
    ]
  editor:
    invisibles: {}
    tabLength: 4
    showIndentGuide: true
    softWrap: true
    softTabs: false
    zoomFontWhenCtrlScrolling: false
    showInvisibles: true
    fontSize: 15
    fontFamily: "Hack"
  whitespace:
    ensureSingleTrailingNewline: true
  "linter-phpcs":
    phpExecutablePath: "/usr/bin/php"
  "one-dark-ui":
    layoutMode: "Spacious"
  "exception-reporting":
    userId: "9645903c-8b1f-6900-4446-96f77aeaf614"
