{
  "*": {
    "atom-beautify": {
      "general": {
        "analytics": false
      }
    },
    "core": {
      "autoHideMenuBar": true,
      "closeDeletedFileTabs": true,
      "disabledPackages": [
        "wrap-guide"
      ],
      "packagesWithKeymapsDisabled": [
        "tool-bar"
      ],
      "telemetryConsent": "no",
      "themes": [
        "nucleus-dark-ui",
        "firewatch-syntax"
      ],
      "uriHandlerRegistration": "always"
    },
    "editor": {
      "fontFamily": "Operator Mono",
      "fontSize": 19,
      "invisibles": {
        "tab": false,
        "eol": false
      },
      "lineHeight": 1.8,
      "scrollPastEnd": true,
      "showIndentGuide": false,
      "showInvisibles": true,
      "showLineNumbers": false,
      "softTabs": false,
      "softWrap": true,
      "tabLength": 4,
      "tabType": "hard",
      "zoomFontWhenCtrlScrolling": false
    },
    "exception-reporting": {
      "userId": "9645903c-8b1f-6900-4446-96f77aeaf614"
    },
    "file-icons": {
      "coloured": false,
      "tabPaneIcon": false
    },
    "line-ending-selector": {
      "defaultLineEnding": "LF"
    },
    "linter": {},
    "linter-eslint": {
      "disableWhenNoEslintConfig": false
    },
    "linter-ui-default": {
      "panelHeight": 60,
      "showPanel": true
    },
    "minimap-plus": {},
    "prettier-atom": {
      "formatOnSaveOptions": {
        "enabled": true,
        "isDisabledIfNotInPackageJson": true,
        "showInStatusBar": true
      },
      "prettierOptions": {
        "useTabs": true
      },
      "useEditorConfig": false,
      "useEslint": true
    },
    "tree-view": {
      "hideIgnoredNames": true
    },
    "wakatime": {
      "apikey": "Saved in your ~/.wakatime.cfg file"
    },
    "welcome": {
      "showOnStartup": false
    },
    "whitespace": {
      "ensureSingleTrailingNewline": true
    }
  }
}
